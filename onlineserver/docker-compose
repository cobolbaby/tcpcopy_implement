services:
  python_app:
    build: 
      context: .
      dockerfile: pydockerfile  
    networks:
      - mdk
    ports:
      - "9999:9999"
    container_name: python_app

  online_server:
    build:
      context: .
      dockerfile: Dockerfile  
    image: inter
    networks:
      - mdk
    container_name: online_server

networks:
  mdk:
    driver: host

   
